/* pages/frontPage.wxss */
.head_view{
  display:flex;
  width: 100%;
  height: 30%;
}
.head_image{
  width: 100%;
}
.body_view{
  width: 100%;
  height: 60%;
  display:flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.bot_view{
  width: 100%;
  display:flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
  margin-top: 30rpx;
}
.icon_container{
  width: 100%;
  height: 220rpx;
  display:flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.icon{
  width: 200rpx;
  height: 220rpx;
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 0 20rpx;
}
.icon image{
  width: 150rpx;
  height: 150rpx;
}